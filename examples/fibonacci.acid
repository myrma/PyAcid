/*
 * Suite de Fibonacci
 */

// n-ième terme de la suite de Fibonacci
(define fib (lambda (n)
  (# (fibs n) (negate 1))
))

// Suite de Fibonacci jusqu'au n-ième terme
(define fibs (lambda (n)
  (fibHelper n 1 1)
))

(define fibHelper (lambda (i x y)
  (if (== i 0)
    (list)
    (append x (fibHelper (- i 1) y (+ x y)))
  )
))
